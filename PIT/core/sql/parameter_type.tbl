create table parameter_type(
   parameter_type_id varchar2(30 char),
   type_description varchar2(255),
   constraint pk_parameter_type primary key (parameter_type_id)
) organization index;

comment on table parameter_type is 'Lookup for parameter types. Used to control the editor that is instantiated to edit parameter values.';
comment on column parameter_type.parameter_type_id is 'Primary key, alphanumeric';
comment on column parameter_type.type_description is 'Longtext of the parameter type';
