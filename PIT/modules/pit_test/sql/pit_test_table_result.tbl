create table pit_test_table_result(
  test_id varchar2(30),
  sort_seq number,
  component varchar2(30),
  params varchar2(200)
);

comment on table pit_test_table_result is 'Table for predefined test results';
comment on column pit_test_table_result.test_id is 'Name of the test the results are collected for';
comment on column pit_test_table_result.sort_seq is 'sort criteria for the result list';
comment on column pit_test_table_result.component is 'Name of the component (PIT_TEST_TABLE.COMPONENT)';
comment on column pit_test_table_result.params is 'Params (PIT_TEST_TABLE.PARAMS)';
